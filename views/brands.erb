<% if @brands.any? %>
<h3 class="cyan-text text-darken-2">All Brands:</h3>
<ul class="cyan-text text-darken-2">
  <% @brands.each() do |brand| %>
    <li><a href="/brands/<%= brand.id %>" class="cyan-text text-darken-2"><%= brand.name() %></a></li>
  <% end %>
</ul>

<% else %>
  <h3 class="cyan-text text-darken-2">There are no brands yet!</h3>
<% end %>
