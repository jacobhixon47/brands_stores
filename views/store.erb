<h1 class="cyan-text"><%= @store.name() %></h1>

<% if @store_brands.any? %>
  <h3 class="cyan-text">Brands In Stock</h3>
  <ul class="cyan-text">
    <% @store_brands.each() do |brand| %>
      <li class="cyan-text"><a href="/brands/<%= brand.id() %>"><%= brand.name() %></a></li>
    <% end %>
  </ul>
<% else %>
  <h3 class="cyan-text">No Brands In Stock</h3>
<% end %>
